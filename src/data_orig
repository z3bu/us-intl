string F1 = "\033[[A"
string F2 = "\033[[B"
string F3 = "\033[[C"
string F4 = "\033[[D"
string F5 = "\033[[E"
string F6 = "\033[17~"
string F7 = "\033[18~"
string F8 = "\033[19~"
string F9 = "\033[20~"
string F10 = "\033[21~"
string F11 = "\033[23~"
string F12 = "\033[24~"
string F13 = "\033[25~"
string F14 = "\033[26~"
string F15 = "\033[28~"
string F16 = "\033[29~"
string F17 = "\033[31~"
string F18 = "\033[32~"
string F19 = "\033[33~"
string F20 = "\033[34~"
string Find = "\033[1~"
string Insert = "\033[2~"
string Remove = "\033[3~"
string Select = "\033[4~"
string Prior = "\033[5~"
string Next = "\033[6~"
string Macro = "\033[M"
string Pause = "\033[P"
## A
compose '\'' 'a' to aacute
compose '`' 'a' to agrave
compose '^' 'a' to acircumflex
compose '"' 'a' to adiaeresis
compose '~' 'a' to atilde
compose '\'' 'A' to Aacute
compose '`' 'A' to Agrave
compose '^' 'A' to Acircumflex
compose '"' 'A' to Adiaeresis
compose '~' 'A' to Atilde
#Dead_ring not existing -> use Altgr + w/W
#compose '°' 'a' to aring
#compose '°' 'A' to Aring
## E
compose '\'' 'e' to eacute
compose '`' 'e' to egrave
compose '^' 'e' to ecircumflex
compose '"' 'e' to ediaeresis
#compose '~' 'e' to etilde #not iso8859-1 not a keysym replace by diaeresis?
compose '\'' 'E' to Eacute
compose '`' 'E' to Egrave
compose '^' 'E' to Ecircumflex
compose '"' 'E' to Ediaeresis
#compose '~' 'E' to Etilde #not iso8859-1 not a keysym replace by diaeresis?
##  I
compose '\'' 'i' to iacute
compose '`' 'i' to igrave
compose '^' 'i' to icircumflex
compose '"' 'i' to idiaeresis
compose '~' 'i' to itilde #not iso8859-1
compose '\'' 'I' to Iacute
compose '`' 'I' to Igrave
compose '^' 'I' to Icircumflex
compose '"' 'I' to Idiaeresis
compose '~' 'I' to Itilde #not iso8859-1
## O
compose '\'' 'o' to oacute
compose '`' 'o' to ograve
