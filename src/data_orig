keycode  10 = nine             parenleft
	alt	keycode  10 = Meta_nine
	altgr keycode  10 = grave #leftsinglequotemark doesn't exist
	shift altgr keycode  10 = dead_breve #synonym to dead_tilde
keycode  11 = zero             parenright       braceright
	alt	keycode  11 = Meta_zero
	altgr keycode  11 = acute #rightsinglequotemark doesn't exist
	#shift altgr keycode  11 = dead_abovering #keysim not existing
keycode  12 = minus            underscore
	shift control keycode 12 = Control_underscore #Undo, restore the command line to what it was before the last edit (since _ is shift-minus on US layouts, this command is actually Ctrl-Shift-Minus)
	alt	keycode  12 = Meta_minus
	altgr keycode  12 = yen
	shift altgr keycode  12 = plusminus #dead_belowdot #keysim not existing
keycode  13 = equal            plus
	alt	keycode  13 = Meta_equal
	altgr keycode  13 = multiply
	shift altgr keycode  13 = division
keycode  14 = Delete           Delete
	control	keycode  14 = BackSpace
	alt	keycode  14 = Meta_Delete
keycode  15 = Tab              Tab
	alt	keycode  15 = Meta_Tab
	shift	keycode  15 = Meta_Tab
keycode  16 = +q	+Q
	control keycode 16 = Control_q #unfreeze display after ctrl_s
	altgr keycode  16 = +adiaeresis
	shift altgr keycode  16 = +Adiaeresis
keycode  17 = +w	+W
	control keycode 17 = Control_w #deletes last word typed
	altgr keycode  17 = +aring
	shift altgr keycode  17 = +Aring
keycode  18 = +e	+E
	control keycode 18 = Control_e #goes to the end of the line
	altgr keycode  18 = +eacute
	shift altgr keycode  18 = +Eacute
keycode  19 = +r	+R
	control keycode 19 = Control_r #reverse search
	altgr keycode  19 = +ediaeresis
	shift altgr keycode  19 = +Ediaeresis
# US version
#	altgr keycode  19 = registered
#	shift altgr keycode  19 = registered
keycode  20 = +t	+T
	control keycode 20 = Control_t #Swap positions of the current and previous chars, and advance one char forward on the command line
	altgr keycode  20 = +thorn
	shift altgr keycode  20 = +THORN
keycode  21 = +y	+Y
	control keycode 21 = Control_y #Copy previously killed text (^W, ^K, ^U, ...) onto the current command line (functioning in effect as cut&paste) 
	altgr keycode  21 = +udiaeresis
	shift altgr keycode  21 = +Udiaeresis
keycode  22 = +u	+U
	control keycode 22 = Control_u #deletes last line typed
	altgr keycode  22 = +uacute
	shift altgr keycode  22 = +Uacute
keycode  23 = +i	+I
	control keycode 23 = Control_i #Move the cursor to the next column divisible by eight (for alignment)/autocomplete commands and file names 
	altgr keycode  23 = +iacute
	shift altgr keycode  23 = +Iacute
keycode  24 = +o	+O
	control keycode 24 = Control_o #Execute the current command and start editing the next history entry (for redoing multiple commands that need small edits)
	altgr keycode  24 = +oacute
	shift altgr keycode  24 = +Oacute
keycode  25 = +p	+P
	control keycode 25 = Control_p #Move to previous (earlier) history entry (same as up-arrow)
	altgr keycode  25 = +odiaeresis
	shift altgr keycode  25 = +Odiaeresis
keycode  26 = bracketleft      braceleft
	control	keycode  26 = Escape #	Used as escape like for sht xkb version but usually a kind of composing character meaning Meta/Alt. ^[5 will repeat the next action five times.
	alt	keycode  26 = Meta_bracketleft
	shift	alt	keycode  26 = Meta_braceleft
	altgr keycode  26 = guillemotleft
	shift altgr keycode  26 = quotedbl #no leftdoublequotemark
keycode  27 = bracketright     braceright
	control	keycode   27 = Control_bracketright #Search char forward, pressing ^] V will place the cursor on the next V on the current command line
	alt	keycode  27 = Meta_bracketright
	shift	alt	keycode  27 = Meta_braceright
	altgr keycode  27 = guillemotright
	shift altgr keycode  27 = quotedbl #no rightdoublequotemark
keycode  28 = Return
	alt	keycode  28 = Meta_Control_m
keycode  29 = Control
keycode  30 = +a	+A
	control keycode 30 = Control_a #GNU screen multifunction key
	altgr keycode  30 = +aacute
	shift altgr keycode  30 = +Aacute
#for french people
#	altgr keycode  30 = +agrave
#	shift altgr keycode  30 = +Agrave
keycode  31 = +s	+S
	control keycode 31 = Control_s	#freeze screen
	altgr keycode  31 = ssharp
	shift altgr keycode  31 = section
keycode  32 = +d	+D
	control keycode 32 = Control_d #ends text input for many 
	altgr keycode  32 = +otilde
	shift altgr keycode  32 = +ETH
keycode  33 = +f	+F
	control keycode 33 = Control_f #One char forward (like right-arrow)
	altgr keycode  33 = +f
	shift altgr keycode  33 = +F
keycode  34 = +g	+G
	control keycode 34 = Control_g #Abort the current editing command (for composite command such as M-5 Up-Arrow (M-5 meaning Meta-5 or Alt+5) which would repeat the Up-Arrow action five times. Pressing ^G after M-5 would abort it, meaning a subsequent Up-Arrow key would just function as normal and just act once)
	altgr keycode  34 = +g
	shift altgr keycode  34 = +G
keycode  35 = +h	+H
	control keycode 35 = Control_h #deletes last character typed
	altgr keycode  35 = +h
	shift altgr keycode  35 = +H
keycode  36 = +j	+J
	control keycode 36 = Control_j #dMove the cursor to the next line*/ Execute the current command line
	altgr keycode  36 = +idiaeresis
	shift altgr keycode  36 = +Idiaeresis
#US version
#	control keycode 36 = Control_j #Move the cursor to the next line*/ Execute the current command line
#	altgr keycode  36 = +j
#	shift altgr keycode  36 = +J
keycode  37 = +k	+K
	control keycode 37 = Control_k #Remove the parts of the command line after the cursor
	altgr keycode  37 = +oe 
	shift altgr keycode  37 = +OE
keycode  38 = +l	+L
	control keycode 38 = Control_l #clear screen
	altgr keycode  38 = +oslash
	shift altgr keycode  38 = +Oslash
keycode  39 = semicolon        colon
	alt	keycode  39 = Meta_semicolon
	altgr keycode  39 = paragraph
	shift altgr keycode  39 = degree
