keycode   8 = seven            ampersand
	alt	keycode   8 = Meta_seven
	altgr keycode  8 = dead_acute #dead_horn not existing
	shift altgr keycode  8 = onehalf
# US version
#	altgr keycode  8 = onehalf
#	shift altgr keycode  8 = dead_acute #dead_horn not existing
keycode   9 = eight            asterisk
