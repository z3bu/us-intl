keycode  42 = Shift
keycode  43 = backslash        bar
	control	keycode  43 = Control_backslash #Goes to the left to the closest word's end
	alt	keycode  43 = Meta_backslash
	shift	alt	keycode  43 = Meta_bar
	altgr keycode  43 = notsign
