# AltGr ist the alt on the right - dead keys are typed with AltGr
keymaps 0-6,8-9,12 #map6 control altgr?
keycode   1 = Escape           Escape
	alt	keycode   1 = Meta_Escape
keycode   2 = one              exclam
	alt	keycode   2 = Meta_one
	shift	alt	keycode   2 = Meta_exclam
	altgr keycode  2 = onesuperior
	shift altgr keycode  2 = exclamdown
# US version
#	altgr keycode  2 = exclamdown
#	shift altgr keycode  2 = onesuperior
keycode   3 = two              at
	alt	keycode   3 = Meta_two
	shift	alt	keycode   3 = Meta_at
	altgr keycode  3 = twosuperior
	shift altgr keycode  3 = dead_doubleacute #looks like a dead_tilde in the end replacement by dead_diaeresis or ´´
keycode   4 = three            numbersign
	#control	keycode   4 = Escape
	alt	keycode   4 = Meta_three
	shift	alt	keycode   4 = Meta_numbersign
	altgr keycode  4 = threesuperior
	shift altgr keycode  4 = macron #dead macron is not existing replace by dead_diaeresis?
keycode   5 = four             dollar
	alt	keycode   5 = Meta_four
	shift	alt	keycode   5 = Meta_dollar
	altgr keycode  5 = currency
	shift altgr keycode  5 = sterling
keycode   6 = five             percent
	alt	keycode   6 = Meta_five
	shift	alt	keycode   6 = Meta_percent
	altgr keycode  6 = euro
	shift altgr keycode  6 = dead_cedilla
keycode   7 = six              circumflex
	control	keycode   7 = Control_asciicircum
	alt	keycode   7 = Meta_six
	shift	alt	keycode   7 = Meta_asciicircum
	altgr keycode  7 = dead_circumflex
	shift altgr keycode  7 = onequarter
# US version
#keycode   7 = six              dead_circumflex
