keycode  45 = +x	+X
	control keycode 45 = Control_x #Bash multifunction key. One command is ^X^E which will invoke the users editor on the current command and execute it when they exit the editor (very useful for long command lines)
	altgr keycode  45 = +oe
	shift altgr keycode  45 = +OE
keycode  46 = +c	+C
	control keycode 46 = Control_c #interrupt program
	altgr keycode  46 = +copyright
	shift altgr keycode  46 = +cent
keycode  47 = +v	+V
	control keycode 47 = Control_v #Insert the next character literally, one can use ^V^S to type a ^S character rather than stop the terminal.
	altgr keycode  47 = registered
	shift altgr keycode  47 = registered
keycode  48 = +b	+B
	control keycode 48 = Control_b #One char backwards (like left-arrow)
	altgr keycode  48 = +b
	shift altgr keycode  48 = +B
keycode  49 = +n	+N
	control keycode 49 = Control_n #Move to next (more recent) history entry (same as down-arrow)
	altgr keycode  49 = +ntilde
	shift altgr keycode  49 = +Ntilde
keycode  50 = +m	+M
	control keycode 50 = Control_m #Move the cursor to the next line* / Execute the current command line
	altgr keycode  50 = mu
	shift altgr keycode  50 = mu
keycode  51 = comma            less
	alt	keycode  51 = Meta_comma
	shift	alt	keycode  51 = Meta_less
	altgr keycode  51 = ccedilla
	shift altgr keycode  51 = Ccedilla
keycode  52 = period           greater
	#control	keycode  52 = Compose
	alt	keycode  52 = Meta_period
	shift	alt	keycode  52 = Meta_greater
	#altgr keycode  52 = dead_abovedot $ not existing
	shift altgr keycode  52 = dead_caron
keycode  53 = slash            question
	#control_slash is not a standard control code - can't be implemented. Goes to the right to the closest word's end (for other US keympas usually abort a running program (SIGQUIT) or a special delete)
	control	keycode  53 =  Control_u # Delete line
	alt	keycode  53 = Meta_slash
	altgr keycode  53 = questiondown
